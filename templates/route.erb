# Static routing configuration for <%= @name %> on <%= @fqdn %>.
#
# WARNING:
#   This file is managed by Puppet via the "<%= @module_name %>" module.

<% @routes.each_with_index do |(k, v), i| -%>
# <%= k %>
ADDRESS<%= i %>=<%= v['address'] %>
NETMASK<%= i %>=<%= v['netmask'] %>

<% end -%>
